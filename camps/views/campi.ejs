<% layout('/layouts/boilerplate') %> 
        <h1>All titles are good</h1>

        <% for(let camp of pro){ %>
        <div class="row">
            <div class="card">
            <div class="row">
                <div class="col-md-4">
                    <img class="img-fluid" alt="cant" src="<%= camp.image%> " >
                </div>
                <div class="col-l-8">
                    <div class="card-body">
                        <h5 class="card-title">
                        <%= camp.title %> 
                        </h5>
                        <h6 class="card-text">
                            <%= camp.description %> 
                        </h6>
                        <p class="card-text">
                            <small class="text-muted">
                                <%= camp.price %> 
                            </small>
                        </p>
                        <a class="btn btn-danger text-muted" href="campgrounds/<%=camp._id%> ">Details</a>
                    </div>
                </div>
            </div>
        </div>
        <% } %> 
       
       <a class="btn btn-primary" href="/campgrounds/new">Add Campground</a>
